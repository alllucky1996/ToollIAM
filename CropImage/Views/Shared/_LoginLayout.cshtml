<!DOCTYPE html>
<!--[if IE 8]> <html lang="en" class="ie8 no-js"> <![endif]-->
<!--[if IE 9]> <html lang="en" class="ie9 no-js"> <![endif]-->
<!--[if !IE]><!-->
<html lang="vi">
<head>
    <title>@ViewBag.Title</title>
    <link rel="shortcut icon" href="~/Theme/adminfav.png" />
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1" name="viewport" />
    <meta content="không cần seo" name="description" />
    <meta content="name" name="Nguyễn Anh Dũng" />
    <title>@ViewBag.Title </title>
    <link href="~/Theme/assets/global/plugins/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Theme/assets/pages/css/login-3.min.css" rel="stylesheet" type="text/css" />
    @Styles.Render("~/Content/css")
    @RenderSection("Styles", required: false)
</head>
<body class=" login">
    <div class="logo">
        <a href="@Url.Action("Index", "Login")">
            <img src="~/Images/logo-default.png" alt="" />
        </a>
    </div>
    <div class="content">
        
        @RenderBody()
    </div>
    <div class="copyright">
        <span>2018-@DateTime.Now.Year</span>
        &copy;<a title="Phòng công nghệ FSIS" href="#">Phòng công nghệ FSIS</a>
    </div>
    <script src="~/Theme/assets/global/plugins/jquery.min.js"></script>
    <script src="~/Theme/assets/global/plugins/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/Theme/assets/global/plugins/js.cookie.min.js"></script>
    <script src="~/Theme/assets/global/plugins/jquery-slimscroll/jquery.slimscroll.min.js"></script>
    <script src="~/Theme/assets/global/plugins/jquery.blockui.min.js"></script>
    <script src="~/Theme/assets/global/plugins/bootstrap-switch/js/bootstrap-switch.min.js"></script>
    <script src="~/Theme/assets/global/scripts/app.min.js"></script>
    <script src="~/Theme/assets/layouts/layout4/scripts/layout.min.js"></script>
    <script src="~/Theme/assets/layouts/layout4/scripts/demo.min.js"></script>
    <script src="~/Theme/assets/layouts/global/scripts/quick-sidebar.min.js"></script>
    <script src="~/Theme/assets/layouts/global/scripts/quick-nav.min.js"></script>
    @RenderSection("scripts", required: false)
    <script type="text/javascript">
        jQuery(document).ready(function () {
            Metronic.init();
            Layout.init();
            Login.init();
            Demo.init();
        });
    </script>
    @RenderSection("scripts2", required: false)
</body>

</html>
